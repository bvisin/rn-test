[ignore]
<PROJECT_ROOT>/node_modules/react-native/.*
<PROJECT_ROOT>/node_modules/config-chain/.*
<PROJECT_ROOT>/node_modules/npm/node_modules/config-chain/.*
<PROJECT_ROOT>/src/.*mocks.*/.*

[include]

[libs]
flow-typed

[lints]

[untyped]

[options]
all=false
emoji=true
module.name_mapper='app'->'<PROJECT_ROOT>/src'
module.name_mapper='conf'->'<PROJECT_ROOT>/config'
module.name_mapper='assets'->'<PROJECT_ROOT>/assets'
module.name_mapper='modules'->'<PROJECT_ROOT>/src/modules'
module.name_mapper='components'->'<PROJECT_ROOT>/src/components'
module.system.node.resolve_dirname=node_modules
